pipeline {
  agent any
  stages {
  
    stage('Run a pwd command') { 
      steps {
        sh 'pwd'
      }
	}
    stage('Run cd ') { 
      steps {
        sh 'cd pipeline-tester-file-1'
      }
    }
    stage('Run an ls command') { 
      steps {
        sh 'ls'
      }
    }

    
    
    stage('Unit Test') { 
      steps {
        sh 'mvn -f ./pipeline-tester-file-1/pom.xml clean test'
      }
    }
  }
}